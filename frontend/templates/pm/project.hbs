<div id="project-{{projectId}}" class="small-square-box cyan-box pm-project">
    <span id="project-setting-{{projectId}}" class="project-setting" title="project settings"></span>    
    <div id="project-logo-{{projectId}}" title="{{projectName}}" class="project-logo">
        {{#if logo}}
        <img title="{{projectName}}" src="{{logo}}" />
        {{/if}}
    </div>
    <span id="project-title-{{projectId}}" title="{{projectName}}" class="project-name">
        <a href="{{url}}"> {{projectName}}</a>
    </span>
</div>
